<AnchorPane id="AnchorPane" prefHeight="577.0" prefWidth="937.0" style="-fx-background-color: #CEF6EC;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pi.esprit.design.homelogin.VideolistController">
   <children>
      <Pane fx:id="PaneVideo" layoutX="279.0" layoutY="77.0" prefHeight="473.0" prefWidth="624.0">
         <children>
            <MediaView fx:id="mediaView" fitHeight="250.0" fitWidth="520.0" layoutX="1.0" />
            <Slider fx:id="volume" layoutX="469.0" layoutY="410.0" />
            <MenuBar layoutX="543.0" layoutY="2.0" prefHeight="25.0" prefWidth="67.0">
               <menus>
                  <Menu fx:id="btn_help" mnemonicParsing="false" onAction="#help" text="close">
                     <items>
                        <MenuItem fx:id="btn_close" mnemonicParsing="false" onAction="#close" text="exit" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <ImageView fx:id="btn_play" fitHeight="50.0" fitWidth="74.0" layoutX="256.0" layoutY="298.0" onMouseClicked="#playvideo" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../images/play.png" />
               </image>
            </ImageView>
            <ImageView fx:id="btn_pause" fitHeight="50.0" fitWidth="74.0" layoutX="162.0" layoutY="298.0" onMouseClicked="#pausevideo" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../images/pause.png" />
               </image>
            </ImageView>
            <ImageView fx:id="btn_stop" fitHeight="50.0" fitWidth="74.0" layoutX="63.0" layoutY="298.0" onMouseClicked="#stopvideo" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../images/stop.png" />
               </image>
            </ImageView>
            <ImageView fx:id="btn_like" fitHeight="25.0" fitWidth="67.0" layoutX="336.0" layoutY="311.0" onMouseClicked="#like" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@like.PNG" />
               </image>
            </ImageView>
            <TextField fx:id="num_like" layoutX="420.0" layoutY="311.0" prefHeight="25.0" prefWidth="29.0" />
            <ImageView fx:id="btn_dislike" fitHeight="25.0" fitWidth="50.0" layoutX="483.0" layoutY="311.0" onMouseClicked="#dislike" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@dislike.png" />
               </image>
            </ImageView>
            <TextField fx:id="num_dislike" layoutX="525.0" layoutY="311.0" prefHeight="25.0" prefWidth="29.0" />
            <ImageView fx:id="btn_comment" fitHeight="42.0" fitWidth="98.0" layoutX="113.0" layoutY="389.0" onMouseClicked="#comment" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../images/commentaire.png" />
               </image>
            </ImageView>
            <TextField fx:id="txt_des" layoutX="187.0" layoutY="398.0" promptText="insert your comment" />
            <Label layoutX="395.0" layoutY="409.0" text="volume" textFill="#2f2d2d" />
            <TextField fx:id="idvid" layoutX="187.0" layoutY="431.0" />
            <TextField fx:id="txt_vid" layoutX="14.0" layoutY="431.0" />
         </children>
      </Pane>
      <ListView fx:id="listvideo" layoutX="14.0" layoutY="57.0" prefHeight="440.0" prefWidth="239.0" />
      <TextField layoutX="182.0" layoutY="4.0" text="Video List">
         <font>
            <Font size="22.0" />
         </font>
      </TextField>
      <Button fx:id="btnbk" layoutX="8.0" layoutY="16.0" mnemonicParsing="false" onAction="#back" text="back" />
      <Button fx:id="like_nb" layoutX="57.0" layoutY="508.0" mnemonicParsing="false" onAction="#like_number" text="number of like" />
      <Button fx:id="dislike_nb" layoutX="50.0" layoutY="538.0" mnemonicParsing="false" onAction="#dislikenumber" text="number of dislike" />
   </children>
</AnchorPane>
